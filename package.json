{
  "name": "macos-bigsur-ui",
  "version": "1.0.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "clean": "rm -rf lib && rm -rf esm && rm -rf dist",
    "install:all": "npm i && cd example && npm i",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:dist",
